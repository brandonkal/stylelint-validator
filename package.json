{
  "name": "@brandonkal/stylelint-csstree-validator",
  "version": "2.5.6",
  "versionUpstream": "1.5.2",
  "description": "Stylelint plugin to validate CSS syntax",
  "homepage": "https://github.com/csstree/validator",
  "author": "Brandon Kalinowski @brandonkal",
  "license": "MIT",
  "repository": "csstree/stylelint-validator",
  "bugs": {
    "url": "https://github.com/csstree/stylelint-validator/issues"
  },
  "main": "index.js",
  "keywords": [
    "stylelint-plugin",
    "stylelint",
    "csstree",
    "css",
    "validator",
    "syntax"
  ],
  "scripts": {
    "test": "tape test | tap-spec",
    "travis": "npm run test"
  },
  "dependencies": {
    "css-tree": "^1.0.0-alpha.34"
  },
  "peerDependencies": {
    "stylelint": ">=7.0.0 <11.0.0"
  },
  "devDependencies": {
    "postcss-jsx": "^0.36.3",
    "postcss-less": "^3.1.4",
    "postcss-scss": "^2.0.0",
    "stylelint": "^10.1.0",
    "stylelint-rule-tester": "^0.6.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-duplicate-case": 2,
      "no-undef": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  },
  "files": [
    "syntax-extension",
    "index.js",
    "LICENSE"
  ]
}
